 #define led 4; // Вибір контакту виводу

void setup() { // Функція конфігурування
  pinMode(led, OUTPUT); // Конфігурація контакту на виведення
}

void loop() { // Нескінченна функція з тілом програми
  digitalWrite(led, HIGH); // Увімкнення виводу. Загоряється світлодіод
  delay(1000); // Затримка
  digitalWrite(led, LOW); // Вимкнення виводу. Вимикається світлодіод
  delay(1000);
}
